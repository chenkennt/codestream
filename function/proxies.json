{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "index": {
      "matchCondition": {
        "route": "/",
        "methods": [
          "GET",
          "OPTIONS"
        ]
      },
      "backendUri": "%ContentRoot%/index.html"
    },
    "scripts": {
      "matchCondition": {
        "route": "/scripts/{*file}",
        "methods": [
          "GET",
          "OPTIONS"
        ]
      },
      "backendUri": "%ContentRoot%/scripts/{file}"
    }
  }
}